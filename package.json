{
  "name": "celeste",
  "version": "0.0.1",
  "description": "celeste",
  "scripts": {
    "test": "mocha --recursive tests/ --reporter dot --stack",
    "wtest": "mocha --recursive tests/ --reporter dot -w --stack",
    "coverage": "istanbul cover _mocha -- --recursive tests/ --reporter dot",
    "coveralls": "istanbul cover _mocha -- --recursive tests/ --reporter dot && istanbul-coveralls --no-rm",
    "start": "nodemon app.js"
  },
  "dependencies": {
    "express": "^4.13.1",
    "lodash": "^3.10.0",
    "node-uuid": "^1.4.3",
    "nodemon": "^1.4.0",
    "q": "^1.2.0",
    "socket.io": "^1.3.4",
    "socket.io-client": "^1.3.4"
  },
  "devDependencies": {
    "chai": "^3.1.0",
    "chai-as-promised": "^5.1.0",
    "istanbul": "^0.3.17",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^2.2.5",
    "mocha-multi": "^0.7.1",
    "mocha-osx-reporter": "^0.1.2",
    "mockery": "^1.4.0",
    "sinon": "^1.15.4"
  }
}
